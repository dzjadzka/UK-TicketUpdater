{
  "name": "uk-ticket-updater",
  "version": "1.1.0",
  "description": "Multi-user NVV semester ticket downloader with device emulation",
  "main": "src/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node src/server.js",
    "download": "node src/index.js --users ./config/users.json",
    "download:sample": "node src/index.js --users ./config/users.sample.json",
    "setup:db": "node src/setupDb.js --source ./config/users.json --db ./data/app.db",
    "download:db": "node src/index.js --db ./data/app.db",
    "api": "node src/server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "pretest": "npm run lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^9.6.0",
    "express": "^5.1.0",
    "puppeteer": "^22.15.0"
  },
  "devDependencies": {
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.1"
  }
}
